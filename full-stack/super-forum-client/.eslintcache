,[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/store/configureStore.ts",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/store/AppState.ts",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/ErrorBoundary.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/serviceWorker.ts",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/routes/Home.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/routes/thread/Thread.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/routes/userProfile/UserProfile.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/routes/thread/ThreadHeader.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/routes/thread/ThreadCategory.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/routes/thread/ThreadTitle.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/routes/thread/ThreadBody.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/routes/thread/ThreadResponsesBuilder.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/auth/common/UserReducer.ts",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/areas/LeftMenu.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/areas/sidebar/SideBar.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/areas/Nav.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/areas/main/Main.tsx",["177"],"import React, { useEffect, useState } from \"react\";\nimport MainHeader from \"./MainHeader\";\nimport { useParams } from \"react-router-dom\";\nimport ThreadCard from \"./ThreadCard\";\nimport { getThreadsByCategory } from \"../../../services/DataService\";\nimport Category from \"../../../models/Category\";\n\ninterface ParamTypes {\n  categoryId: string;\n}\nconst Main = () => {\n  const { categoryId } = useParams<ParamTypes>();\n  const [category, setCategory] = useState<Category | undefined>();\n  const [threadCards, setThreadCards] = useState<Array<JSX.Element> | null>(\n    null\n  );\n\n  useEffect(() => {\n    console.log(\"main categoryId\", categoryId);\n\n    if (categoryId && parseInt(categoryId) > 0) {\n      getThreadsByCategory(categoryId).then((threads) => {\n        const cards = threads.map((th) => {\n          return <ThreadCard key={`thread-${th.id}`} thread={th} />;\n        });\n        if (!category) {\n          setCategory(threads[0].category);\n        }\n        setThreadCards(cards);\n      });\n    }\n  }, [categoryId]);\n\n  return (\n    <main className=\"content\">\n      <MainHeader category={category} />\n      <div>{threadCards}</div>\n    </main>\n  );\n};\n\nexport default Main;\n","/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/areas/rightMenu/RightMenu.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/points/ThreadPointsBar.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/auth/common/PasswordComparison.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/models/CategoryThread.ts",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/auth/common/Helpers.ts",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/routes/thread/UserNameAndTime.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/routes/thread/ThreadResponse.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/areas/sidebar/SideBarMenus.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/areas/main/MainHeader.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/areas/main/ThreadCard.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/areas/rightMenu/TopCategory.tsx",[],"/home/chakraborty/web/react/react/full-stack/super-forum-client/src/components/editor/RichEditor.tsx",["178"],"import React, { useState, useCallback, useMemo, useEffect, FC } from \"react\";\nimport { Editable, withReact, useSlate, Slate } from \"slate-react\";\nimport { Editor, Transforms, createEditor, Node } from \"slate\";\nimport isHotkey from \"is-hotkey\";\nimport { withHistory } from \"slate-history\";\nimport { Button, Toolbar } from \"./RichTextControls\";\nimport { FontAwesomeIcon } from \"@fortawesome/react-fontawesome\";\nimport {\n  faBold,\n  faItalic,\n  faUnderline,\n  faCode,\n  faHeading,\n  faQuoteRight,\n  faListOl,\n  faListUl,\n} from \"@fortawesome/free-solid-svg-icons\";\nimport \"./RichEditor.css\";\n\nconst HOTKEYS: { [keyName: string]: string } = {\n  \"mod+b\": \"bold\",\n  \"mod+i\": \"italic\",\n  \"mod+u\": \"underline\",\n  \"mod+`\": \"code\",\n};\nconst initialValue = [\n  {\n    type: \"paragraph\",\n    children: [{ text: \"Enter your post here.\" }],\n  },\n];\nconst LIST_TYPES = [\"numbered-list\", \"bulleted-list\"];\n\ninterface RichEditorProps {\n  existingBody?: string;\n}\n\nconst RichEditor: FC<RichEditorProps> = ({ existingBody }) => {\n  const [value, setValue] = useState<Node[]>(initialValue);\n  const renderElement = useCallback((props) => <Element {...props} />, []);\n  const renderLeaf = useCallback((props) => <Leaf {...props} />, []);\n  const editor = useMemo(() => withHistory(withReact(createEditor())), []);\n\n  useEffect(() => {\n    if (existingBody) {\n      setValue([\n        {\n          type: \"paragraph\",\n          text: existingBody,\n        },\n      ]);\n    }\n  }, []);\n\n  const onChangeEditorValue = (val: Node[]) => {\n    setValue(val);\n  };\n\n  return (\n    <Slate editor={editor} value={value} onChange={onChangeEditorValue}>\n      <Toolbar>\n        <MarkButton format=\"bold\" icon=\"bold\" />\n        <MarkButton format=\"italic\" icon=\"italic\" />\n        <MarkButton format=\"underline\" icon=\"underlined\" />\n        <MarkButton format=\"code\" icon=\"code\" />\n        <BlockButton format=\"heading-one\" icon=\"header1\" />\n        <BlockButton format=\"block-quote\" icon=\"in_quotes\" />\n        <BlockButton format=\"numbered-list\" icon=\"list_numbered\" />\n        <BlockButton format=\"bulleted-list\" icon=\"list_bulleted\" />\n      </Toolbar>\n      <Editable\n        className=\"editor\"\n        renderElement={renderElement}\n        renderLeaf={renderLeaf}\n        placeholder=\"Enter some rich text…\"\n        spellCheck\n        autoFocus\n        onKeyDown={(event) => {\n          for (const hotkey in HOTKEYS) {\n            if (isHotkey(hotkey, event as any)) {\n              event.preventDefault();\n              const mark = HOTKEYS[hotkey];\n              toggleMark(editor, mark);\n            }\n          }\n        }}\n      />\n    </Slate>\n  );\n};\n\nconst MarkButton = ({ format, icon }: { format: string; icon: string }) => {\n  const editor = useSlate();\n  let thisIcon = faBold;\n  if (icon === \"italic\") {\n    thisIcon = faItalic;\n  } else if (icon === \"underlined\") {\n    thisIcon = faUnderline;\n  } else if (icon === \"code\") {\n    thisIcon = faCode;\n  }\n  return (\n    <Button\n      active={isMarkActive(editor, format)}\n      onMouseDown={(event: any) => {\n        event.preventDefault();\n        toggleMark(editor, format);\n      }}\n    >\n      <FontAwesomeIcon icon={thisIcon} />\n    </Button>\n  );\n};\n\nconst isMarkActive = (editor: Editor, format: string) => {\n  const marks = Editor.marks(editor);\n  return marks ? marks[format] === true : false;\n};\n\nconst toggleMark = (editor: Editor, format: string) => {\n  const isActive = isMarkActive(editor, format);\n\n  if (isActive) {\n    Editor.removeMark(editor, format);\n  } else {\n    Editor.addMark(editor, format, true);\n  }\n};\n\nconst BlockButton = ({ format, icon }: { format: string; icon: string }) => {\n  const editor = useSlate();\n  let thisIcon = faHeading;\n  if (icon === \"heading1\") {\n    thisIcon = faItalic;\n  } else if (icon === \"heading2\") {\n    thisIcon = faUnderline;\n  } else if (icon === \"in_quotes\") {\n    thisIcon = faQuoteRight;\n  } else if (icon === \"list_numbered\") {\n    thisIcon = faListOl;\n  } else if (icon === \"list_bulleted\") {\n    thisIcon = faListUl;\n  }\n  return (\n    <Button\n      active={isBlockActive(editor, format)}\n      onMouseDown={(event: any) => {\n        event.preventDefault();\n        toggleBlock(editor, format);\n      }}\n    >\n      <FontAwesomeIcon icon={thisIcon} />\n    </Button>\n  );\n};\n\nconst isBlockActive = (editor: Editor, format: string) => {\n  const [match] = Editor.nodes(editor, {\n    match: (n) => n.type === format,\n  });\n\n  return !!match;\n};\n\nconst toggleBlock = (editor: Editor, format: string) => {\n  const isActive = isBlockActive(editor, format);\n  const isList = LIST_TYPES.includes(format);\n\n  Transforms.unwrapNodes(editor, {\n    match: (n) => LIST_TYPES.includes(n.type as string),\n    split: true,\n  });\n\n  Transforms.setNodes(editor, {\n    type: isActive ? \"paragraph\" : isList ? \"list-item\" : format,\n  });\n\n  if (!isActive && isList) {\n    const block = { type: format, children: [] };\n    Transforms.wrapNodes(editor, block);\n  }\n};\n\nconst Element = ({\n  attributes,\n  children,\n  element,\n}: {\n  attributes: any;\n  children: any;\n  element: any;\n}) => {\n  switch (element.type) {\n    case \"block-quote\":\n      return <blockquote {...attributes}>{children}</blockquote>;\n    case \"bulleted-list\":\n      return <ul {...attributes}>{children}</ul>;\n    case \"heading-one\":\n